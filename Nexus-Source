  Window:Dialog({
      Title = "مرحبآ بك", 
      Text = "اتمني ان يعجبك السكربت ",
      Options = {
          {"قفل", function()  
              if MainFrame:FindFirstChild("Dialog") then
                  MainFrame.Dialog:Destroy()
              end
          end},
          {"لاحقاً", function()
              if MainFrame:FindFirstChild("Dialog") then
                  MainFrame.Dialog:Destroy()
              end
          end}
      }
  })
end)

