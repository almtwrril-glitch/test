SpyTab:AddParagraph({"ØªØ¬Ø³Ø³ Ù…ØªÙ‚Ø¯Ù…", "ØªØ¬Ø³Ø³ Ù…Ø¨Ø§Ø´Ø± Ø£Ùˆ ØªÙ„Ù‚Ø§Ø¦ÙŠ Ø¹Ù„Ù‰ Ø§Ù„Ù„Ø§Ø¹Ø¨ÙŠÙ†"})

SpyTab:AddDropdown({
    Name = "ØªØ¬Ø³Ø³ Ù…Ø¨Ø§Ø´Ø± Ø¹Ù„Ù‰ Ù„Ø§Ø¹Ø¨",
    Options = (function()
        local t = {}
        for _, v in ipairs(Players:GetPlayers()) do
            if v ~= LocalPlayer then table.insert(t, v.Name) end
        end
        return t
    end)(),
    Callback = function(v)
        local p = Players:FindFirstChild(v)
        if p then startSpy(p) end
    end,
})

SpyTab:AddButton({
    Name = "Ø¥ÙŠÙ‚Ø§Ù Ø§Ù„ØªØ¬Ø³Ø³",
    Callback = function()
        stopSpy()
    end,
})

SpyTab:AddToggle({
    Name = "ØªØ¬Ø³Ø³ ØªÙ„Ù‚Ø§Ø¦ÙŠ (ØªÙ†Ø§ÙˆØ¨)",
    Default = false,
    Callback = function(value)
        autoSpyEnabled = value
        if value then
            task.spawn(function()
                while autoSpyEnabled do
                    local allPlayers = Players:GetPlayers()
                    for _, player in pairs(allPlayers) do
                        if not autoSpyEnabled then break end
                        if player ~= LocalPlayer and player.Character then
                            startSpy(player)
                            task.wait(5)
                        end
                    end
                    if autoSpyEnabled then task.wait(1) end
                end
                stopSpy()
            end)
        else
            stopSpy()
        end
    end,
})
