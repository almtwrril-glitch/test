local seatEnabled = false
local seats = {}

ProtectTab:AddToggle({
    Name = "Ø­Ù…Ø§ÙŠØ© Ø¬Ù„ÙˆØ³",
    Default = false,
    Callback = function(state)
        seats = {}
        for _, seat in next, workspace:GetDescendants() do
            if seat:IsA("Seat") then
                seats[seat] = true
            end
        end
        workspace.DescendantAdded:Connect(function(seat)
            if seat:IsA("Seat") then
                seats[seat] = true
                seat.Disabled = not seatEnabled
            end
        end)
        workspace.DescendantRemoving:Connect(function(seat)
            if seat:IsA("Seat") then
                seats[seat] = nil
            end
        end)
        seatEnabled = not state
        for seat, _ in next, seats do
            seat.Disabled = not seatEnabled
        end
