for _, v in ipairs(musicList) do
    local name, id = v[1], v[2]
    MusicTab:AddButton({
        Name = name,
        Callback = function()
            playMusic(id)
            Notify("Ø§Ù„Ø§ØºØ§Ù†ÙŠ", name .. "!", 3)
        end
    })
end

