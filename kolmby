local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")
local Players = game:GetService("Players")
local Player = Players.LocalPlayer
local PlayerGui = Player:WaitForChild("PlayerGui")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local LocalPlayer = Players.LocalPlayer
local Workspace = game:GetService("Workspace")

-- ===== Ù…ØªØºÙŠØ±Ø§Øª Ø¹Ø§Ù…Ø© =====
local foundPlayer = nil
local faceSitEnabled = false
local bangEnabled = false
local bangTarget = nil
local spyEnabled = false
local spyTarget = nil
local autoSpyEnabled = false
local originalCameraSubject = nil

local function resetVel()
    pcall(function()
        if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
            LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.zero
        end
    end)
end

-- ===== Ø§Ù„Ø§Ù†ØªØ±Ùˆ Ø§Ù„Ø£ÙˆÙ„ (Kamui) =====
local function PlayKamuiIntro()
    local screenGui1 = Instance.new("ScreenGui")
    screenGui1.IgnoreGuiInset = true
    screenGui1.Parent = PlayerGui

    local bg1 = Instance.new("Frame")
    bg1.Size = UDim2.new(1,0,1,0)
    bg1.BackgroundColor3 = Color3.fromRGB(0,0,0)
    bg1.BorderSizePixel = 0
    bg1.Parent = screenGui1

    local img1 = Instance.new("ImageLabel")
    img1.Size = UDim2.new(0,0,0,0)
    img1.Position = UDim2.new(0.5,0,0.5,0)
    img1.AnchorPoint = Vector2.new(0.5,0.5)
    img1.BackgroundTransparency = 1
    img1.Image = "rbxassetid://13879097036"
    img1.ScaleType = Enum.ScaleType.Fit
    img1.ImageTransparency = 0
    img1.BorderSizePixel = 0
    img1.Parent = screenGui1

    local snd = Instance.new("Sound")
    snd.SoundId = "rbxassetid://1592708450"
    snd.Parent = Player.Character or Player:WaitForChild("Character")
    snd:Play()

    local duration = 4.2
    local growTime = 2
    local rotSpeed = 0
    local maxSpeed = 12
    local accel = 0.2
    local decelTime = 1.5

    task.spawn(function()
        local st = tick()
        while img1.Parent do
            local el = tick() - st
            if el < (duration - decelTime) then
                if rotSpeed < maxSpeed then rotSpeed = rotSpeed + accel end
            else
                if rotSpeed > 0 then rotSpeed = rotSpeed - accel end
            end
            img1.Rotation = img1.Rotation + rotSpeed
            task.wait(0.016)
        end
    end)

    task.spawn(function()
        local st = tick()
        while tick() - st < growTime do
            local p = (tick() - st) / growTime
            img1.Size = UDim2.new(p,0,p,0)
            task.wait(0.01)
        end
        img1.Size = UDim2.new(1,0,1,0)
    end)

    task.wait(duration)
    screenGui1:Destroy()
end

-- ===== Ø´Ø§Ø´Ø© Ø§Ù„ØªØ­Ù…ÙŠÙ„ =====
local function PlayLoadingScreen()
    local LoadingConfig = {
        Duration = 6,
        FadeOutTime = 1.2,
    }

    local Particles = {}
    local AnimConns = {}

    local screenGui2 = Instance.new("ScreenGui")
    screenGui2.IgnoreGuiInset = true
    screenGui2.Parent = PlayerGui

    local MainFrame = Instance.new("Frame")
    MainFrame.Size = UDim2.new(1,0,1,0)
    MainFrame.BackgroundColor3 = Color3.fromRGB(0,0,0)
    MainFrame.BorderSizePixel = 0
    MainFrame.ZIndex = 10
    MainFrame.Name = "MainFrame"
    MainFrame.Parent = screenGui2

    local bg = Instance.new("Frame")
    bg.Size = UDim2.new(1,0,1,0)
    bg.BackgroundColor3 = Color3.fromRGB(0,0,0)
    bg.BorderSizePixel = 0
    bg.ZIndex = 1
    bg.Parent = MainFrame
    local g = Instance.new("UIGradient")
    g.Color = ColorSequence.new({
        ColorSequenceKeypoint.new(0, Color3.fromRGB(80,0,0)),
        ColorSequenceKeypoint.new(0.3, Color3.fromRGB(0,0,0)),
        ColorSequenceKeypoint.new(0.7, Color3.fromRGB(60,0,0)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(0,0,0))
    })
    g.Rotation = 45
    g.Parent = bg
    local ga = TweenService:Create(g, TweenInfo.new(4, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1, true), {Rotation=225})
    ga:Play(); table.insert(AnimConns, ga)

    for i = 1, 5 do
        local c = Instance.new("Frame")
        c.Size = UDim2.new(0,100+(i*50),0,100+(i*50))
        c.Position = UDim2.new(0.5,-(50+(i*25)),0.5,-(50+(i*25)))
        c.BackgroundColor3 = Color3.fromRGB(180,0,0)
        c.BackgroundTransparency = 0.87
        c.BorderSizePixel = 0
        c.ZIndex = 3
        c.Parent = MainFrame
        Instance.new("UICorner", c).CornerRadius = UDim.new(1,0)
        local pt = TweenService:Create(c, TweenInfo.new(2+(i*0.3), Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1, true),
            {Size=UDim2.new(0,130+(i*60),0,130+(i*60)), Position=UDim2.new(0.5,-(65+(i*30)),0.5,-(65+(i*30))), BackgroundTransparency=0.96})
        pt:Play(); table.insert(AnimConns, pt)
    end

    local function MakeParticle(x, y)
        local s = Instance.new("Frame")
        s.Size = UDim2.new(0, math.random(2,3), 0, math.random(2,3))
        s.Position = UDim2.new(0,x,0,y)
        s.BackgroundColor3 = Color3.fromRGB(255,255,255)
        s.BorderSizePixel = 0
        s.ZIndex = 8
        s.BackgroundTransparency = math.random(0,50)/100
        s.Parent = MainFrame
        Instance.new("UICorner", s).CornerRadius = UDim.new(1,0)
        return {frame=s, sx=(math.random(-100,100)/100)*2, sy=(math.random(-100,100)/100)*2,
            rs=math.random(-5,5), ps=math.random(1,3), op=math.random(30,100)/100}
    end

    local sv = workspace.CurrentCamera.ViewportSize
    for i = 1, 120 do
        table.insert(Particles, MakeParticle(math.random(0,sv.X), math.random(0,sv.Y)))
    end

    local pc = RunService.Heartbeat:Connect(function()
        local csz = workspace.CurrentCamera.ViewportSize
        for _, p in pairs(Particles) do
            if p.frame and p.frame.Parent then
                local nx = p.frame.Position.X.Offset + p.sx
                local ny = p.frame.Position.Y.Offset + p.sy
                if nx < -10 then nx = csz.X+10 elseif nx > csz.X+10 then nx = -10 end
                if ny < -10 then ny = csz.Y+10 elseif ny > csz.Y+10 then ny = -10 end
                p.frame.Position = UDim2.new(0,nx,0,ny)
                p.frame.Rotation = p.frame.Rotation + p.rs
                local pv = math.sin(tick()*p.ps)*0.3+0.7
                p.frame.BackgroundTransparency = 1-(p.op*pv)
            end
        end
    end)
    table.insert(AnimConns, pc)

    local glow = Instance.new("Frame")
    glow.Size = UDim2.new(0.45,0,0.45,0)
    glow.Position = UDim2.new(0.5,0,0.28,0)
    glow.AnchorPoint = Vector2.new(0.5,0.5)
    glow.BackgroundColor3 = Color3.fromRGB(180,0,0)
    glow.BackgroundTransparency = 0.85
    glow.BorderSizePixel = 0
    glow.ZIndex = 9
    glow.Parent = MainFrame
    Instance.new("UICorner", glow).CornerRadius = UDim.new(1,0)

    local imgL = Instance.new("ImageLabel")
    imgL.Size = UDim2.new(0,0,0,0)
    imgL.Position = UDim2.new(0.5,0,0.28,0)
    imgL.AnchorPoint = Vector2.new(0.5,0.5)
    imgL.BackgroundColor3 = Color3.fromRGB(0,0,0)
    imgL.BackgroundTransparency = 0
    imgL.Image = "rbxassetid://110381872691213"
    imgL.ScaleType = Enum.ScaleType.Fit
    imgL.ImageTransparency = 1
    imgL.BorderSizePixel = 0
    imgL.ZIndex = 11
    imgL.Parent = MainFrame
    local asp = Instance.new("UIAspectRatioConstraint")
    asp.AspectRatio = 1
    asp.DominantAxis = Enum.DominantAxis.Height
    asp.Parent = imgL
    Instance.new("UICorner", imgL).CornerRadius = UDim.new(0.03,0)
    local imgStroke = Instance.new("UIStroke")
    imgStroke.Color = Color3.fromRGB(200,0,0)
    imgStroke.Thickness = 3
    imgStroke.Transparency = 1
    imgStroke.Parent = imgL

    local nameBg = Instance.new("Frame")
    nameBg.Size = UDim2.new(0.5,0,0,44)
    nameBg.Position = UDim2.new(0.5,0,0.55,0)
    nameBg.AnchorPoint = Vector2.new(0.5,0)
    nameBg.BackgroundColor3 = Color3.fromRGB(0,0,0)
    nameBg.BackgroundTransparency = 1
    nameBg.BorderSizePixel = 0
    nameBg.ZIndex = 12
    nameBg.Parent = MainFrame
    Instance.new("UICorner", nameBg).CornerRadius = UDim.new(0.2,0)
    local nameBgStroke = Instance.new("UIStroke")
    nameBgStroke.Color = Color3.fromRGB(180,0,0)
    nameBgStroke.Thickness = 2
    nameBgStroke.Transparency = 1
    nameBgStroke.Parent = nameBg
    local nameLbl = Instance.new("TextLabel")
    nameLbl.Size = UDim2.new(1,0,1,0)
    nameLbl.BackgroundTransparency = 1
    nameLbl.Text = "KOLMBY INTRO"
    nameLbl.TextColor3 = Color3.fromRGB(210,0,0)
    nameLbl.TextTransparency = 1
    nameLbl.Font = Enum.Font.GothamBold
    nameLbl.TextScaled = true
    nameLbl.ZIndex = 13
    nameLbl.Parent = nameBg

    local barBg = Instance.new("Frame")
    barBg.Size = UDim2.new(0.5,0,0,20)
    barBg.Position = UDim2.new(0.5,0,0.65,0)
    barBg.AnchorPoint = Vector2.new(0.5,0)
    barBg.BackgroundColor3 = Color3.fromRGB(20,20,20)
    barBg.BorderSizePixel = 0
    barBg.ZIndex = 12
    barBg.Parent = MainFrame
    Instance.new("UICorner", barBg).CornerRadius = UDim.new(0,10)
    local barStroke = Instance.new("UIStroke")
    barStroke.Color = Color3.fromRGB(180,0,0)
    barStroke.Thickness = 2
    barStroke.Transparency = 0.3
    barStroke.Parent = barBg
    local bsp = TweenService:Create(barStroke, TweenInfo.new(1.5, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1, true), {Transparency=0.05})
    bsp:Play(); table.insert(AnimConns, bsp)

    local bar = Instance.new("Frame")
    bar.Size = UDim2.new(0,0,1,0)
    bar.BackgroundColor3 = Color3.fromRGB(180,0,0)
    bar.BorderSizePixel = 0
    bar.ZIndex = 13
    bar.Parent = barBg
    Instance.new("UICorner", bar).CornerRadius = UDim.new(0,10)
    local barG = Instance.new("UIGradient")
    barG.Color = ColorSequence.new({
        ColorSequenceKeypoint.new(0, Color3.fromRGB(150,0,0)),
        ColorSequenceKeypoint.new(0.5, Color3.fromRGB(255,60,60)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(150,0,0))
    })
    barG.Parent = bar
    local bga = TweenService:Create(barG, TweenInfo.new(2, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1, true), {Offset=Vector2.new(1,0)})
    bga:Play(); table.insert(AnimConns, bga)

    local glowEff = Instance.new("Frame")
    glowEff.Size = UDim2.new(0,60,1,0)
    glowEff.Position = UDim2.new(-0.2,0,0,0)
    glowEff.BackgroundColor3 = Color3.fromRGB(255,255,255)
    glowEff.BackgroundTransparency = 0.6
    glowEff.BorderSizePixel = 0
    glowEff.ZIndex = 14
    glowEff.Parent = bar
    Instance.new("UICorner", glowEff).CornerRadius = UDim.new(0,10)

    local pctLbl = Instance.new("TextLabel")
    pctLbl.Size = UDim2.new(0.5,0,0,28)
    pctLbl.Position = UDim2.new(0.5,0,0.72,0)
    pctLbl.AnchorPoint = Vector2.new(0.5,0)
    pctLbl.BackgroundTransparency = 1
    pctLbl.Text = "0%"
    pctLbl.TextColor3 = Color3.fromRGB(210,0,0)
    pctLbl.TextTransparency = 1
    pctLbl.Font = Enum.Font.GothamBold
    pctLbl.TextScaled = true
    pctLbl.ZIndex = 12
    pctLbl.Parent = MainFrame

    local devLbl = Instance.new("TextLabel")
    devLbl.Size = UDim2.new(0.5,0,0,26)
    devLbl.Position = UDim2.new(0.5,0,0.80,0)
    devLbl.AnchorPoint = Vector2.new(0.5,0)
    devLbl.BackgroundTransparency = 1
    devLbl.Text = "Ø§Ù„Ù…Ø·ÙˆØ±: Ø§Ø­Ù…Ø¯ (ÙƒÙˆÙ„Ù…Ø¨ÙŠ)"
    devLbl.TextColor3 = Color3.fromRGB(180,0,0)
    devLbl.TextTransparency = 1
    devLbl.Font = Enum.Font.GothamBold
    devLbl.TextScaled = true
    devLbl.ZIndex = 12
    devLbl.Parent = MainFrame

    local gm = TweenService:Create(glowEff, TweenInfo.new(1.2, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1, true),
        {Position=UDim2.new(1.3,0,0,0)})
    gm:Play(); table.insert(AnimConns, gm)

    TweenService:Create(glow, TweenInfo.new(1.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
        {Size=UDim2.new(0.42,0,0.42,0), BackgroundTransparency=0.82}):Play()
    TweenService:Create(imgL, TweenInfo.new(1.4, Enum.EasingStyle.Back, Enum.EasingDirection.Out),
        {Size=UDim2.new(0,340,0,340), ImageTransparency=0}):Play()
    TweenService:Create(imgStroke, TweenInfo.new(1.4, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
        {Transparency=0}):Play()

    task.wait(1.2)
    TweenService:Create(nameBg, TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundTransparency=0}):Play()
    TweenService:Create(nameBgStroke, TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Transparency=0}):Play()
    TweenService:Create(nameLbl, TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {TextTransparency=0}):Play()
    TweenService:Create(pctLbl, TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {TextTransparency=0}):Play()
    TweenService:Create(devLbl, TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {TextTransparency=0}):Play()

    task.spawn(function()
        while screenGui2.Parent do
            TweenService:Create(imgL, TweenInfo.new(1.6, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut),
                {Position=UDim2.new(0.5,0,0.26,0)}):Play()
            task.wait(1.6)
            TweenService:Create(imgL, TweenInfo.new(1.6, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut),
                {Position=UDim2.new(0.5,0,0.30,0)}):Play()
            task.wait(1.6)
        end
    end)

    task.spawn(function()
        while screenGui2.Parent do
            TweenService:Create(glow, TweenInfo.new(0.9, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut),
                {BackgroundTransparency=0.65, Size=UDim2.new(0.46,0,0.46,0)}):Play()
            task.wait(0.9)
            TweenService:Create(glow, TweenInfo.new(0.9, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut),
                {BackgroundTransparency=0.88, Size=UDim2.new(0.40,0,0.40,0)}):Play()
            task.wait(0.9)
        end
    end)

    local startT = tick()
    local function updateBar()
        local el = tick() - startT
        local prog = math.min(el / LoadingConfig.Duration, 1)
        local pct = math.floor(prog * 100)
        TweenService:Create(bar, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
            {Size=UDim2.new(prog,0,1,0)}):Play()
        pctLbl.Text = pct .. "%"

        if prog >= 1 then
            pctLbl.Text = "100%"
            TweenService:Create(bar, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
                {BackgroundColor3=Color3.fromRGB(255,80,80)}):Play()
            task.wait(0.5)
            for _, c in pairs(AnimConns) do
                if typeof(c) == "RBXScriptConnection" then c:Disconnect()
                elseif typeof(c) == "Tween" then c:Cancel() end
            end
            for _, p in pairs(Particles) do
                if p.frame then p.frame:Destroy() end
            end
            TweenService:Create(MainFrame, TweenInfo.new(LoadingConfig.FadeOutTime, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
                {BackgroundTransparency=1, Size=UDim2.new(1.3,0,1.3,0), Position=UDim2.new(-0.15,0,-0.15,0)}):Play()
            task.wait(LoadingConfig.FadeOutTime)
            screenGui2:Destroy()
            return
        end
        RunService.Heartbeat:Wait()
        updateBar()
    end
    updateBar()
end

